; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[  ]
_global_script_class_icons={

}

[application]

config/name="Tetrim"
config/description="A Tetris clone with a relaxing color palette"
run/main_scene="res://TitleScreen.tscn"
boot_splash/bg_color=Color( 0.156863, 0.156863, 0.156863, 1 )
config/icon="res://icon.png"

[autoload]

StoreSettings="*res://StoreSettings.gd"
BackgroundMusic="*res://BackgroundMusic.tscn"
Utils="*res://Utils.gd"

[debug]

settings/fps/force_fps=60

[display]

window/size/width=640
window/size/height=700
window/handheld/orientation="portrait"
window/stretch/mode="2d"
window/stretch/aspect="expand"

[logging]

file_logging/enable_file_logging=true
file_logging/max_log_files=10

[rendering]

environment/default_environment="res://default_env.tres"
